(var
  ops           [+ -]
  op-count      (len ops)
  op-bits       (bit/len op-count)
  value-bits    (- 8 op-bits)
  base          8
  max-value     (pow base (- value-bits 1))
  values        (list/alloc (+ max-value 1)))

(values 0 0)
(values 1 1)

(say max-value)

(let [v 1
      i 1]
  (for [_ (range 0 value-bits 1)]
    (say i " " v)
    (values v (bit/>> i op-bits))
    (set v (* v base)
         i (* i 2))))